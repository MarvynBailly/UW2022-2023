   RowBox[{
    RowBox[{"y", "[", "t_", "]"}], " ", ":=", " ", 
    RowBox[{
     RowBox[{
      SubscriptBox["y", "0"], "[", "t", "]"}], " ", "+", " ", 
     RowBox[{"\[Epsilon]", "*", 
      RowBox[{
       SubscriptBox["y", "1"], "[", "t", "]"}]}]}]}]}]}]], "Code",
