13265fed835d"],

Cell[BoxData[
 RowBox[{"ODEi", " ", "=", " ", 
  RowBox[{"ODEii", " ", "/", " ", 
   RowBox[{"(", 
    RowBox[{"\[Epsilon]", "/", 
     RowBox[{"\[Delta]", "^", "2"}]}], ")"}]}]}]], "Code",
