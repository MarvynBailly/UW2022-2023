   3.8872914515293827`*^9}, {3.8872915564529886`*^9, 3.887291562351289*^9}, {
   3.8874027179785557`*^9, 3.8874027180878983`*^9}, {3.8874027501351547`*^9, 
   3.8874027783507195`*^9}, {3.887405889061391*^9, 3.887405890169297*^9}, {
