Cell[TextData[{
 "Compute the ODE for boundary conditions at ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       StyleBox["a", "TI"], TraditionalForm], "errors" -> {}, "input" -> "a", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "84c5513b-6343-42c0-bcee-9045eb925e18"],
 " and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       StyleBox["b", "TI"], TraditionalForm], "errors" -> {}, "input" -> "b", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "02b8c79c-2de2-4f82-966b-3a1499ab8a45"]
}], "Text",
