Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"\[Epsilon]", " ", "=", " ", "0.1"}], ";"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{"sol", " ", "=", " ", 
   RowBox[{"NDSolve", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"ODE", " ", "==", " ", "0"}], ",", " ", 
       RowBox[{"BCa", " ", "==", " ", "0"}], ",", " ", 
       RowBox[{"BCb", " ", "==", " ", "0"}]}], "}"}], ",", " ", 
     RowBox[{"y", "[", "x", "]"}], ",", " ", "x"}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"num", "[", "x_", "]"}], " ", ":=", " ", 
    RowBox[{"Evaluate", "[", 
     RowBox[{
      RowBox[{"y", "[", "x", "]"}], "/.", " ", 
      RowBox[{"sol", "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], "]"}]}], ";"}], " "}], "\n", 
 RowBox[{
  RowBox[{"Plot", "[", 
   RowBox[{
    RowBox[{"num", "[", "x", "]"}], ",", " ", 
    RowBox[{"{", 
     RowBox[{"x", ",", "a", ",", "b"}], "}"}]}], "]"}], "\n"}], "\n", 
 RowBox[{"Clear", "[", "\[Epsilon]", "]"}]}], "Code",
