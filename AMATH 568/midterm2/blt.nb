  3.8870346959040995`*^9, 3.8870347244469814`*^9}, {3.887034755839471*^9, 
  3.887034772093007*^9}},ExpressionUUID->"a3e77ddb-c18a-49f2-a949-\
b86789355c00"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"a", " ", "=", " ", "0"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"b", " ", "=", " ", 
    RowBox[{"Pi", "/", "2"}]}], ";"}], " ", "\n"}], "\n", 
 RowBox[{"ODE", " ", "=", " ", 
  RowBox[{
   RowBox[{"\[Epsilon]", "*", 
    RowBox[{
     RowBox[{"y", "''"}], "[", "x", "]"}]}], " ", "-", " ", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"1", "/", 
      RowBox[{"(", 
       RowBox[{"4", " ", "+", " ", 
        RowBox[{"Sin", "[", "x", "]"}]}], ")"}]}], ")"}], " ", 
    RowBox[{
     RowBox[{"y", "'"}], "[", "x", "]"}]}], " ", "+", "  ", 
   RowBox[{"y", "[", "x", "]"}]}]}], "\n", 
 RowBox[{"BCa", " ", "=", " ", 
  RowBox[{
   RowBox[{"y", "[", "a", "]"}], " ", "-", " ", "1"}]}], "\n", 
 RowBox[{"BCb", " ", "=", " ", 
  RowBox[{
   RowBox[{"y", "[", "b", "]"}], " ", "-", " ", "1"}]}]}], "Code",
 CellChangeTimes->{{3.887034623721746*^9, 3.8870346645858345`*^9}, {
   3.8870348817770257`*^9, 3.8870349018414683`*^9}, 3.8870417492454386`*^9, 
   3.8870420204384766`*^9, {3.8872912081774335`*^9, 3.887291210271247*^9}, {
